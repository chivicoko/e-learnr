{
    "headers": [
    {
        "source": "/(.*).(jpg|jpeg|png|gif|mp4)$",
        "headers": [
            {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
            }
        ]
        }
    ],
    "rewrites": [
        { "source": "/(.*)","destination": "/" }
    ]
}
